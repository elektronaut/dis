{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "release-type": "ruby",
      "version-file": "lib/dis/version.rb",
      "extra-files": [
        {
          "type": "generic",
          "path": "Gemfile.lock",
          "regex": "dis \\((?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)\\)"
        }
      ]
    }
  }
}
